#include <stdio.h>
#include <iostream>

#define LINE_LENGTH 35

int main(){
  char actor[] = "Judy Garland";
  int age = 17;
  char role[] = "Dorothy";
  int line;

  puts("Wizard of Oz Database!\n");

  printf("%-15s\t%3s\t%-15s\n", "Actor", "Age", "Role");
  for(line=0;line<LINE_LENGTH; line++) putchar('-');
  utchar('\n');

  printf("%-15s\t%3d\t%-15s\n",actor,age,role);

  return 0; 

}
